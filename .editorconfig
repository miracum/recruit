root = true

[*]
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_trailing_newline = true

[*.java]
indent_style = space
indent_size = 2
continuation_indent_size = 4

[*.bat]
end_of_line = crlf
charset = utf-8

[*.{js,jsx,ts,tsx,vue}]
indent_style = space
indent_size = 2
trim_trailing_whitespace = true
insert_final_newline = true
max_line_length = 128
